{
  "name": "aditum",
  "version": "2.0.5",
  "description": "A accessible component library to manage focus in React single page applications",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npx babel ./src --watch --out-dir ./dist",
    "test": "jest --verbose",
